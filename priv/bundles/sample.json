{
	"searchTweets": {
		"resource": "spashttp.request",
		"params": {
			"url": "http://search.twitter.com/search.json",
			"q": "dtex",
			"lang": "en",
			"count": 100 
		},
		"cacheduration": 3600,
		"timeout": 500,
		"filter": {
			"results": [{
				"from_user": true,
				"text": true }]
		}
	},
	"searchMoreTweets": {
		"resource": "spashttp.request",
		"params": {
			"url": "http://search.twitter.com/search.json",
			"q":"spas",
			"lang": "en" 
		},
		"cacheduration": 3600
	}
}